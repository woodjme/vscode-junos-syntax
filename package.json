{
    "name": "junos",
    "displayName": "Junos Syntax",
    "description": "Junos syntax highlighting for VSCode",
    "version": "1.3.0",
    "publisher": "jamiewoodio",
    "icon": "images/junos.png",
    "engines": {
        "vscode": "^1.5.0"
    },
